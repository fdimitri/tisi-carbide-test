<p id="notice"><%= notice %></p>

<h1>Listing Ticket Updates</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Desc</th>
      <th>Ticket</th>
      <th>User</th>
      <th>Assigneduser</th>
      <th>State</th>
      <th>Priority</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ticket_updates.each do |ticket_update| %>
      <tr>
        <td><%= ticket_update.title %></td>
        <td><%= ticket_update.desc %></td>
        <td><%= ticket_update.Ticket %></td>
        <td><%= ticket_update.User %></td>
        <td><%= ticket_update.AssignedUser %></td>
        <td><%= ticket_update.State %></td>
        <td><%= ticket_update.Priority %></td>
        <td><%= link_to 'Show', ticket_update %></td>
        <td><%= link_to 'Edit', edit_ticket_update_path(ticket_update) %></td>
        <td><%= link_to 'Destroy', ticket_update, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ticket update', new_ticket_update_path %>
